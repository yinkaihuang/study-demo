<?xml version="1.0" encoding="UTF-8" ?>
<!--代码构建mybatis配置需要用到-->
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.bucheng.mybatis.mapper.OrderMapper"> <!-- 注意，因为这边没有用到mapper接口，所以这里的namespace不需要是完全的类名 -->

    <select id="findAll" resultType="cn.bucheng.mybatis.domain.Order">
        select * from t_order;
    </select>

    <select id="findAllNoProxy" resultType="cn.bucheng.mybatis.domain.Order">
        select * from t_order_0;
    </select>
    
</mapper>